{
  "name": "LumynLabs-cx",
  "version": "1.0.0",
  "description": "Lumyn Labs SDK for ConnectorX",
  "keywords": [
    "connectorx",
    "led",
    "animation",
    "robotics",
    "frc",
    "rp2040"
  ],
  "authors": [
    {
      "name": "Lumyn Labs",
      "url": "https://lumynlabs.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumynlabs/connectorx-sdk"
  },
  "license": "Proprietary",
  "frameworks": [
    "arduino"
  ],
  "platforms": [
    "raspberrypi"
  ],
  "build": {
    "flags": [
      "-I include",
      "-I include/FastLED"
    ],
    "libArchive": false,
    "extraScript": "link_sdk.py"
  },
  "dependencies": {
    "FreeRTOS": "*",
    "LittleFS": "*",
    "SDFS": "*",
    "SPI": "*",
    "Wire": "*",
    "EEPROM": "*",
    "adafruit/Adafruit TinyUSB Library": "^3.4.0"
  },
  "export": {
    "include": [
      "include/*",
      "lib/*"
    ],
    "exclude": [
      "src/*",
      ".pio/*"
    ]
  }
}
